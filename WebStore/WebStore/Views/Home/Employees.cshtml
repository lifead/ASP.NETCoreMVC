@model IEnumerable<WebStore.Models.EmployeeView>
@using WebStore.Models

<div class="container">
    <table  class="table table-bordered table-striped"  >
        <thead>
            <tr>
                <th>Id</th>
                <th>Фамилия Имя Отчество</th>
                <th>Возраст</th>
                <th></th>

            </tr>
        </thead>
        <tbody>
            @foreach (EmployeeView item in Model)
            {
            <tr>
                <td>@item.EmployeeId</td>
                <td>@item.FIO</td>
                <td>@item.Age</td>
                <td><a  asp-controller="Home" asp-action="Details" asp-route-employeeId="@item.EmployeeId">Карточка</a></td>
            </tr>
            }
        </tbody>
    </table>
</div>
